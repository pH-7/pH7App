<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest>
    <application generateAndroid="true" generateiOS="false" version="50.0">
        <main>main.php</main>
        <services>http://localhost/services/</services>
        <applicationTitle>Hello World</applicationTitle>
           <!-- Adds -->
           <EnableAds/> <!-- True - False -->
           <AdsProvider/> <!-- AdMob - Mobfox - SmartAd -->
           <!-- Analytics -->
           <EnableAnalytics/> <!-- True - False -->
           <GoogleAnalyticsTrackerId/>
           <!-- Twitter -->
           <TwitterAppKey/>
           <TwitterAppSecret/>
           <!-- Facebook -->
           <FacebookAppId/>
         <!-- Notifications -->
          <EnableNotification/> <!-- True - False -->

        <android layoutOrientation="Portrait" navigationStyle="Default">
            <Theme>Android</Theme>
            <versionCode>1.0</versionCode>
            <versionName>1.0</versionName>
            <packagedName>com.kikapp.ph7app</packagedName>
            <lunchImage>launchimage.png</lunchImage>
            <applicationIcon>appicon.png</applicationIcon>
            <BaseStyle/> <!-- Default - Dark - Light - Light with Dark Action Bar -->
