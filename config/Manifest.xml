            <AdMobPublisherId/>
            <!-- Android Maps API Key -->
            <MapAPIKey/>
            <!-- YouTube -->
            <AndroidYoutubeAPIKey/>
            <!-- Android sender Id - Notifications. -->
            <AndroidSenderId/>
            <AndroidSenderAPIKey/>
        </android>

        <iOS layoutOrientation="Portrait" navigationStyle="Default">
            <Theme>iOSTheme</Theme>
            <versionCode>1.0</versionCode>
            <versionName>1.0</versionName>
            <BundleIdentifier>com.kikapp.ph7app</BundleIdentifier>
            <Devices>Universal</Devices> <!-- Universal - iPhone - iPad  -->
            <applicationIcon>appicon.png</applicationIcon>
            <lunchImage>launchimage.png</lunchImage>
            <ExecuteType>KikAppEmulator</ExecuteType> <!-- Compile - KikAppEmulator -->
            <LocationAlwaysUsage/>
            <LocationWhenInUseUsage/>
            <AllowsArbitraryLoads>True</AllowsArbitraryLoads> <!-- True - False -->
        </iOS>

       </application>
</manifest>